# Solana Configuration
# Solana RPC URL - use Helius, QuickNode, or other Solana RPC provider
NEXT_PUBLIC_SOLANA_RPC_URL=https://devnet.helius-rpc.com

# Irys Configuration for metadata storage
NEXT_PUBLIC_IRYS_URL=https://devnet.irys.xyz

# Volcengine Seedream API Configuration (required for meme generation)
# Get from: https://console.volcengine.com/ark-platform/ark/apiKey
# Use either SEEDREAM_API_* or VOLCENGINE_API_* as they are interchangeable
SEEDREAM_API_AK=your-volcengine-access-key
SEEDREAM_API_SK=your-volcengine-secret-key
# Alternative naming convention
VOLCENGINE_API_AK=your-volcengine-access-key
VOLCENGINE_API_SK=your-volcengine-secret-key
# Optional Volcengine configuration
VOLCENGINE_ENDPOINT=https://visual.volcengineapi.com/
VOLCENGINE_REGION=cn-north-1

# Helius API Configuration (required for metadata upload and RPC)
# Get from: https://www.helius.xyz/
HELIUS_API_KEY=your-helius-api-key
NEXT_PUBLIC_HELIUS_API_KEY=your-helius-api-key

# Backend Configuration
PORT=3005
NODE_ENV=development

# Frontend Configuration
# Backend URL for frontend to connect to
NEXT_PUBLIC_BACKEND_URL=http://localhost:3005

# Optional: Minting Wallet Configuration (for backend minting)
# SECRET_KEY=your-base58-encoded-secret-key (only needed if using backend minting)
